  "DataService": [
    {
      "_id": "_suri:InfotecAdmin:DataService:LogService",
      "id": "LogService",
      "created": "2019-01-02",
      "creator": "softjei@gmail.com",
      "service": "function(request, response, dataSource, action){\n  if(dataSource!==\"Log\")\n  {\n\n    var data={\n      source:this.source,\n      //user:this.user.login,\n      dataSource:dataSource,\n      action:action,\n      data:JSON.stringify(request.data),\n      date:new java.util.Date()\n    };\n    if(this.user)\n    {\n      data.user=this.user.email;\n      data.userIp=this.user.ip;\n    }\n    this.getDataSource(\"Log\").addObj(data);\n  }\n}",
      "dataSources": [
        "*"
      ],
      "actions": [
        "add",
        "update",
        "remove"
      ],
      "updated": "2019-01-02T19:55:59.813Z",
      "updater": "softjei@gmail.com"
    }
  ],
